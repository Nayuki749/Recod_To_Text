<UserControl x:Class="Recod_To_Text.VersionInfomationWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Recod_To_Text"
             mc:Ignorable="d" 
              Width="220" Height="120"
             Background="White" >
    <Grid>
        <TextBlock x:Name="textBlock_Recod_To_Text_Ver" HorizontalAlignment="Left" Margin="20,21,0,0" TextWrapping="Wrap" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Recod_To_Text_Ver, FallbackValue='Recode_To_Text_Ver:0.0.0.0'}" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock_Speech_To_Text_DLL_Ver" HorizontalAlignment="Left" Margin="20,42,0,0" TextWrapping="Wrap" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Speech_To_Text_Ver, FallbackValue='Speech_To_Text_DLL_Ver:0.0.0.0'}" VerticalAlignment="Top"/>
        <Button x:Name="button_OK" Content="OK" HorizontalAlignment="Left" Margin="129,81,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.447,1.563" Click="Button_OK_Click"/>
    </Grid>
</UserControl>
